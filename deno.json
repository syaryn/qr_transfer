{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check main.ts",
    "start": "deno run -A --watch=static/,locales/,main.ts main.ts"
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "hono": "npm:hono@4.6.9",
    "hono/": "npm:hono@4.6.9/",
    "hono/html": "npm:hono/html",
    "hono/deno": "npm:hono/deno",
    "qrcode": "npm:qrcode@1.5.3",
    "svgo": "npm:svgo@3.2.0",
    "$std/": "https://deno.land/std@0.216.0/"
  },
  "nodeModulesDir": "auto"
}
